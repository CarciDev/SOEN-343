model EarthLocation {
  id                      Int             @id @default(autoincrement())
  createdAt               DateTime        @default(now())
  updatedAt               DateTime        @updatedAt
  address1                String?
  address2                String?
  city                    String?
  /// Code assigned by ISO 3166-2, without the country part
  administartiveArea      String?
  /// With spaces, in the form of the respective country
  postalCode              String?
  /// ISO 3166-1 2-letter country code
  countryCode             String?         @db.Char(2)
  lat                     Float
  lng                     Float
  quotationsAsDestination Quotation[]     @relation("destinationId")
  quotationsAsOrigin      Quotation[]     @relation("originId")
  trackingEvents          TrackingEvent[]
}
